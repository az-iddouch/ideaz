<div class="pagination">
  <div class="pagination__prev">
    {{#checkIfSup page 1}}
    <a href="/categories/page/{{prevPage page}}?categorie={{categorie._id}}">
      <svg class="feather">
        <use xlink:href="/icons/feather-sprite.svg#chevron-left" />
      </svg>
    </a>
    {{/checkIfSup}}
  </div>
  <div class="pagination__text">
    <p>Page {{page}} of {{pages}} - {{count}} total results</p>
  </div>
  <div class="pagination__next">
    {{#checkIfInf page pages}}
    <a href="/categories/page/{{nextPage page}}?categorie={{categorie._id}}">
      <svg class="feather ">
        <use xlink:href="/icons/feather-sprite.svg#chevron-right" />
      </svg>
    </a>
    {{/checkIfInf}}
  </div>
</div>